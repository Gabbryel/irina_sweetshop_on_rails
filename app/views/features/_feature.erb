<% if isUserAdmin? %>
  <div class="dashboard-card">
    <p><%= feature.title %></p>
    <p><%= feature.content %></p>
    <p><%= cl_image_tag(feature.photo.key, width: 100) %></p>
    <p><%= link_to 'edit', edit_feature_path(feature) %> <%= link_to 'del', feature_path(feature), method: :delete %></p>
  </div>
<% end %>